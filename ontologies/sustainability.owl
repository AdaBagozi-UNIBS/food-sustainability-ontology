@prefix nc-f: <https://knowledge.next-cart.com/food#> .
@prefix nc-s: <https://knowledge.next-cart.com/sustainability#> .
@prefix nc-fsc: <https://knowledge.next-cart.com/foodSupplyChain#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fo: <http://purl.obolibrary.org/obo/> .
@prefix tb: <https://w3id.org/timebank/> .
@prefix dbo: <http://dbpedia.org/ontology/> .

nc-s: a owl:Ontology ;
    <http://purl.org/dc/elements/1.1/creator> "Anisa Rula" , "Ada Bagozi" ;
    <http://purl.org/dc/elements/1.1/contributor> "Devis Bianchini" , "Michele Melchiori" ;
    owl:versionIRI <https://knowledge.next-cart.com/sustainability/1.0.0> ;
    <http://purl.org/dc/elements/1.1/publisher> "UNIBS" ;
    <http://purl.org/dc/elements/1.1/title> "The NextCart Ontology - Sustainability" ;
    <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
    <http://purl.org/vocab/vann/preferredNamespacePrefix> "nc-s" ;
    <http://purl.org/vocab/vann/preferredNamespaceUri> "https://knowledge.next-cart.com/sustainability#" ;
    owl:versionInfo "1.0.0" .

# Classes

nc-fsc:Stakeholder a owl:Class ;
    rdfs:comment "Represents stakeholders in the food supply chain." .

nc-fsc:Distributor a owl:Class ;
    rdfs:subClassOf nc-fsc:Stakeholder ;
    rdfs:comment "Represents distributors in the food supply chain." .

nc-fsc:Retailer a owl:Class ;
    rdfs:subClassOf nc-fsc:Stakeholder ;
    rdfs:comment "Represents retailers in the food supply chain." .

nc-fsc:CertificationAuthority a owl:Class ;
    rdfs:subClassOf nc-fsc:Stakeholder ;
    rdfs:comment "Represents certification authorities in the food supply chain." .

nc-fsc:Producer a owl:Class ;
    rdfs:subClassOf nc-fsc:Stakeholder ;
    rdfs:comment "Represents producers in the food supply chain." .

nc-fsc:Farmer a owl:Class ;
    rdfs:subClassOf nc-fsc:Stakeholder ;
    rdfs:comment "Represents farmers in the food supply chain." .

nc-fsc:FoodSupplyChain a owl:Class ;
    rdfs:comment "Represents the food supply chain." .

nc-s:Sustainability a owl:Class ;
    rdfs:comment "Represents the sustainability aspects." .

nc-s:EnvironmentalSustainability a owl:Class ;
    rdfs:subClassOf nc-s:Sustainability ;
    rdfs:comment "Aspects related to the environmental impact of food production and consumption." .

nc-s:WaterFootprint a owl:Class ;
    rdfs:subClassOf nc-s:EnvironmentalSustainability ;
    rdfs:comment "The amount of water used in the production of food." .

nc-s:CarbonFootprint a owl:Class ;
    rdfs:subClassOf nc-s:EnvironmentalSustainability ;
    rdfs:comment "The total greenhouse gases emitted during food production." .

nc-s:EnergyUse a owl:Class ;
    rdfs:subClassOf nc-s:EnvironmentalSustainability ;
    rdfs:comment "Energy consumed in various stages of the food supply chain (FSC)." .

nc-s:BiodiversityImpact a owl:Class ;
    rdfs:subClassOf nc-s:EnvironmentalSustainability ;
    rdfs:comment "Effects of food production on biodiversity." .

nc-s:EconomicSustainability a owl:Class ;
    rdfs:subClassOf nc-s:Sustainability ;
    rdfs:comment "Financial aspects affecting the food supply chain (FSC)." .

nc-s:CostEfficiency a owl:Class ;
    rdfs:subClassOf nc-s:EconomicSustainability ;
    rdfs:comment "Measures to reduce costs and improve economic viability." .

nc-s:FairTrade a owl:Class ;
    rdfs:subClassOf nc-s:EconomicSustainability ;
    rdfs:comment "Ensuring fair prices and wages for producers." .

nc-s:SocialSustainability a owl:Class ;
    rdfs:subClassOf nc-s:Sustainability ;
    rdfs:comment "Social implications of food production and consumption." .

nc-s:FoodSecurity a owl:Class ;
    rdfs:subClassOf nc-s:SocialSustainability ;
    rdfs:comment "Ensuring all people have access to sufficient, safe, and nutritious food." .

nc-s:LaborConditions a owl:Class ;
    rdfs:subClassOf nc-s:SocialSustainability ;
    rdfs:comment "Working conditions of individuals involved in the FSC." .

nc-s:CommunityImpact a owl:Class ;
    rdfs:subClassOf nc-s:SocialSustainability ;
    rdfs:comment "Effects on local communities." .

nc-s:WasteManagement a owl:Class ;
    rdfs:subClassOf nc-s:Sustainability ;
    rdfs:comment "Aspects related to reducing and managing waste within the FSC." .

nc-s:FoodWaste a owl:Class ;
    rdfs:subClassOf nc-s:WasteManagement ;
    rdfs:comment "Wastage occurring at various stages of the FSC." .

nc-s:Recycling a owl:Class ;
    rdfs:subClassOf nc-s:WasteManagement ;
    rdfs:comment "Practices related to recycling and reuse of food products and packaging." .

nc-s:Composting a owl:Class ;
    rdfs:subClassOf nc-s:WasteManagement ;
    rdfs:comment "Organic waste management through composting." .

# Object Properties

nc-s:hasSustainability a owl:ObjectProperty ;
    rdfs:domain nc-fsc:FoodSupplyChain ;    
    rdfs:label "has sustainability" ;
    rdfs:range nc-s:Sustainability .

nc-fsc:isStakeholderIn a owl:ObjectProperty ;
    rdfs:domain nc-fsc:Stakeholder ;
    rdfs:range nc-fsc:FoodSupplyChain .

nc-fsc:processedIn a owl:ObjectProperty ;
    rdfs:domain nc-f:FoodItem ;
    rdfs:range nc-fsc:FoodSupplyChain .

nc-s:impacts a owl:ObjectProperty ;
    rdfs:domain nc-fsc:Stakeholder ;
    rdfs:range nc-s:Sustainability .

# Data Properties

nc-s:expirationDate a owl:DatatypeProperty ;
    rdfs:domain nc-f:FoodItem ;
    rdfs:range xsd:dateTime .

# Food Supply Chain Instances
nc-fsc:SeafoodSupplyChain a nc-fsc:FoodSupplyChain ;
    rdfs:label "Sustainable Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:SustainableSeafoodSustainability .

nc-fsc:DairySupplyChain a nc-fsc:FoodSupplyChain ;
    rdfs:label "Organic Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:OrganicDairySustainability .

# Stakeholders for Seafood Supply Chain
nc-fsc:FishermanCooperative a nc-fsc:Farmer ;
    rdfs:label "Fisherman Cooperative" ;
    rdfs:comment "A cooperative of local fishermen practicing sustainable fishing." ;
    nc-fsc:isStakeholderIn nc-fsc:SeafoodSupplyChain ;
    nc-s:impacts nc-s:WaterFootprint_FishermanCooperative, nc-s:BiodiversityImpact_FishermanCooperative .

nc-fsc:SeafoodDistributor a nc-fsc:Distributor ;
    rdfs:label "Sustainable Seafood Distributors" ;
    rdfs:comment "A distributor specializing in sustainably sourced seafood." ;
    nc-fsc:isStakeholderIn nc-fsc:SeafoodSupplyChain ;
    nc-s:impacts nc-s:EnergyUse_SeafoodDistributor, nc-s:CarbonFootprint_SeafoodDistributor .

nc-fsc:SeafoodRetailer a nc-fsc:Retailer ;
    rdfs:label "Sustainable Seafood Retailer" ;
    rdfs:comment "A retailer focused on selling sustainable seafood products." ;
    nc-fsc:isStakeholderIn nc-fsc:SeafoodSupplyChain ;
    nc-s:impacts nc-s:FoodWaste_SeafoodRetailer, nc-s:Recycling_SeafoodRetailer .

# Stakeholders for Dairy Supply Chain
nc-fsc:DairyFarm a nc-fsc:Producer ;
    rdfs:label "Organic Dairy Farm" ;
    rdfs:comment "A farm producing organic dairy products." ;
    nc-fsc:isStakeholderIn nc-fsc:DairySupplyChain ;
    nc-s:impacts nc-s:WaterFootprint_DairyFarm, nc-s:CarbonFootprint_DairyFarm .

nc-fsc:DairyDistributor a nc-fsc:Distributor ;
    rdfs:label "Organic Dairy Distributor" ;
    rdfs:comment "A distributor specializing in organic dairy products." ;
    nc-fsc:isStakeholderIn nc-fsc:DairySupplyChain ;
    nc-s:impacts nc-s:EnergyUse_DairyDistributor, nc-s:CarbonFootprint_DairyDistributor .

nc-fsc:DairyRetailer a nc-fsc:Retailer ;
    rdfs:label "Organic Dairy Retailer" ;
    rdfs:comment "A retailer focused on selling organic dairy products." ;
    nc-fsc:isStakeholderIn nc-fsc:DairySupplyChain ;
    nc-s:impacts nc-s:FoodWaste_DairyRetailer, nc-s:Recycling_DairyRetailer, nc-s:Composting_DairyRetailer .

# Sustainability Aspects for Seafood Supply Chain
nc-s:SustainableSeafoodSustainability a nc-s:Sustainability ;
    rdfs:label "Sustainability of Sustainable Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:EnvironmentalSustainability_Seafood, nc-s:EconomicSustainability_Seafood, nc-s:SocialSustainability_Seafood, nc-s:WasteManagement_Seafood .

nc-s:EnvironmentalSustainability_Seafood a nc-s:EnvironmentalSustainability ;
    rdfs:label "Environmental Sustainability in Sustainable Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:WaterFootprint_FishermanCooperative, nc-s:BiodiversityImpact_FishermanCooperative .

nc-s:WaterFootprint_FishermanCooperative a nc-s:WaterFootprint ;
    rdfs:label "Water Footprint of Fisherman Cooperative" ;
    rdfs:comment "Water usage in the fishing practices of the Fisherman Cooperative." .

nc-s:BiodiversityImpact_FishermanCooperative a nc-s:BiodiversityImpact ;
    rdfs:label "Biodiversity Impact of Fisherman Cooperative" ;
    rdfs:comment "Impact of fishing practices on marine biodiversity." .

nc-s:EconomicSustainability_Seafood a nc-s:EconomicSustainability ;
    rdfs:label "Economic Sustainability in Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:CostEfficiency_Seafood, nc-s:FairTrade_Seafood .

nc-s:CostEfficiency_Seafood a nc-s:CostEfficiency ;
    rdfs:label "Cost Efficiency in Seafood Supply Chain" ;
    rdfs:comment "Measures to reduce costs and improve economic viability in the seafood supply chain." .

nc-s:FairTrade_Seafood a nc-s:FairTrade ;
    rdfs:label "Fair Trade Practices in Seafood Supply Chain" ;
    rdfs:comment "Ensuring fair prices and wages for fishermen." .

nc-s:SocialSustainability_Seafood a nc-s:SocialSustainability ;
    rdfs:label "Social Sustainability in Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:FoodSecurity_Seafood, nc-s:LaborConditions_Seafood, nc-s:CommunityImpact_Seafood .

nc-s:FoodSecurity_Seafood a nc-s:FoodSecurity ;
    rdfs:label "Food Security in Seafood Supply Chain" ;
    rdfs:comment "Ensuring access to safe and nutritious seafood products." .

nc-s:LaborConditions_Seafood a nc-s:LaborConditions ;
    rdfs:label "Labor Conditions in Seafood Supply Chain" ;
    rdfs:comment "Working conditions of individuals involved in the seafood supply chain." .

nc-s:CommunityImpact_Seafood a nc-s:CommunityImpact ;
    rdfs:label "Community Impact of Seafood Supply Chain" ;
    rdfs:comment "Effects of the seafood supply chain on local communities." .

nc-s:WasteManagement_Seafood a nc-s:WasteManagement ;
    rdfs:label "Waste Management in Seafood Supply Chain" ;
    nc-s:hasSustainability nc-s:FoodWaste_SeafoodRetailer, nc-s:Recycling_SeafoodRetailer .

nc-s:FoodWaste_SeafoodRetailer a nc-s:FoodWaste ;
    rdfs:label "Food Waste Management at Seafood Retailer" ;
    rdfs:comment "Practices to reduce food waste at the seafood retailer." .

nc-s:Recycling_SeafoodRetailer a nc-s:Recycling ;
    rdfs:label "Recycling Practices at Seafood Retailer" ;
    rdfs:comment "Recycling and reuse of packaging materials at the seafood retailer." .

# Sustainability Aspects for Dairy Supply Chain
nc-s:OrganicDairySustainability a nc-s:Sustainability ;
    rdfs:label "Sustainability of Organic Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:EnvironmentalSustainability_Dairy, nc-s:EconomicSustainability_Dairy, nc-s:SocialSustainability_Dairy, nc-s:WasteManagement_Dairy .

nc-s:EnvironmentalSustainability_Dairy a nc-s:EnvironmentalSustainability ;
    rdfs:label "Environmental Sustainability in Organic Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:WaterFootprint_DairyFarm, nc-s:CarbonFootprint_DairyFarm .

nc-s:WaterFootprint_DairyFarm a nc-s:WaterFootprint ;
    rdfs:label "Water Footprint of Dairy Farm" ;
    rdfs:comment "Water usage in the production of organic dairy products at the dairy farm." .

nc-s:CarbonFootprint_DairyFarm a nc-s:CarbonFootprint ;
    rdfs:label "Carbon Footprint of Dairy Farm" ;
    rdfs:comment "Greenhouse gas emissions from dairy farming operations at the dairy farm." .

nc-s:EconomicSustainability_Dairy a nc-s:EconomicSustainability ;
    rdfs:label "Economic Sustainability in Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:CostEfficiency_Dairy, nc-s:FairTrade_Dairy .

nc-s:CostEfficiency_Dairy a nc-s:CostEfficiency ;
    rdfs:label "Cost Efficiency in Dairy Supply Chain" ;
    rdfs:comment "Measures to reduce costs and improve economic viability in the dairy supply chain." .

nc-s:FairTrade_Dairy a nc-s:FairTrade ;
    rdfs:label "Fair Trade Practices in Dairy Supply Chain" ;
    rdfs:comment "Ensuring fair prices and wages for dairy farmers." .

nc-s:SocialSustainability_Dairy a nc-s:SocialSustainability ;
    rdfs:label "Social Sustainability in Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:FoodSecurity_Dairy, nc-s:LaborConditions_Dairy, nc-s:CommunityImpact_Dairy .

nc-s:FoodSecurity_Dairy a nc-s:FoodSecurity ;
    rdfs:label "Food Security in Dairy Supply Chain" ;
    rdfs:comment "Ensuring access to safe and nutritious dairy products." .

nc-s:LaborConditions_Dairy a nc-s:LaborConditions ;
    rdfs:label "Labor Conditions in Dairy Supply Chain" ;
    rdfs:comment "Working conditions of individuals involved in the dairy supply chain." .

nc-s:CommunityImpact_Dairy a nc-s:CommunityImpact ;
    rdfs:label "Community Impact of Dairy Supply Chain" ;
    rdfs:comment "Effects of the dairy supply chain on local communities." .

nc-s:WasteManagement_Dairy a nc-s:WasteManagement ;
    rdfs:label "Waste Management in Dairy Supply Chain" ;
    nc-s:hasSustainability nc-s:FoodWaste_DairyRetailer, nc-s:Recycling_DairyRetailer, nc-s:Composting_DairyRetailer .

nc-s:FoodWaste_DairyRetailer a nc-s:FoodWaste ;
    rdfs:label "Food Waste Management at Dairy Retailer" ;
    rdfs:comment "Practices to reduce food waste at the dairy retailer." .

nc-s:Recycling_DairyRetailer a nc-s:Recycling ;
    rdfs:label "Recycling Practices at Dairy Retailer" ;
    rdfs:comment "Recycling and reuse of packaging materials at the dairy retailer." .

nc-s:Composting_DairyRetailer a nc-s:Composting ;
    rdfs:label "Composting at Dairy Retailer" ;
    rdfs:comment "Composting of organic waste at the dairy retailer." .

# Food Items
nc-f:OrganicMilk a nc-f:FoodItem ;
    rdfs:label "Organic Milk" ;
    rdfs:comment "Fresh organic milk produced by the Organic Dairy Farm." ;
    nc-fsc:processedIn nc-fsc:DairySupplyChain ;
    nc-s:expirationDate "2024-07-20T23:59:59"^^xsd:dateTime .

nc-f:OrganicCheese a nc-f:FoodItem ;
    rdfs:label "Organic Cheese" ;
    rdfs:comment "Artisan organic cheese produced by the Organic Dairy Farm." ;
    nc-fsc:processedIn nc-fsc:DairySupplyChain ;
    nc-s:expirationDate "2024-08-15T23:59:59"^^xsd:dateTime .

nc-f:SustainableSalmon a nc-f:FoodItem ;
    rdfs:label "Sustainable Salmon" ;
    rdfs:comment "Fresh sustainable salmon caught by the Fisherman Cooperative." ;
    nc-fsc:processedIn nc-fsc:SeafoodSupplyChain ;
    nc-s:expirationDate "2024-07-25T23:59:59"^^xsd:dateTime .

nc-f:SustainableShrimp a nc-f:FoodItem ;
